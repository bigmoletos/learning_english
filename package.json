    "ajv": "^6.12.6",
    "ajv-keywords": "^3.5.2",
    "cypress": "^15.6.0",
    "jest-environment-jsdom": "^30.2.0",
    "msw": "^2.12.1",
    "node-fetch": "^3.3.2",
    "source-map-explorer": "^2.5.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.9.1",
    "whatwg-fetch": "^3.6.20"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:ci": "jest --config jest.config.js --ci --coverage --maxWorkers=2",
    "test:backend": "cd backend && npm install sequelize winston express-rate-limit && npm test",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "npm run test:coverage && npm run test:backend && npm run test:e2e",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/*.js",
    "generate:corpus": "node scripts/generateCorpus.js",
    "train:agent": "ts-node src/agents/trainAgent.ts",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx,.js,.jsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^5.2.0",
    "glob": "^11.0.0"
  }
