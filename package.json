{
  "name": "ai-english-trainer-it",
  "version": "1.0.0",
  "private": true,
  "description": "Outil d'entraînement en anglais technique (IA/DevOps) avec agent IA, reconnaissance vocale, et préparation TOEIC/TOEFL (B2→C1).",
  "dependencies": {
    "@capacitor-community/text-to-speech": "^6.1.0",
    "@capacitor/android": "^7.4.4",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/network": "^7.0.2",
    "@capacitor/preferences": "^7.0.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.10",
    "@mui/material": "^5.14.10",
    "@mui/x-charts": "^6.0.0",
    "axios": "^1.6.2",
    "firebase": "^12.5.0",
    "i18next": "^23.7.11",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-router-dom": "^6.18.0",
    "react-scripts": "^5.0.1",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.14.200",
    "@types/natural": "^5.1.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-syntax-highlighter": "^15.5.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "ajv": "^6.12.6",
    "ajv-keywords": "^3.5.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:chrome": "react-scripts start && pwsh -ExecutionPolicy Bypass -File scripts/open-browser.ps1",
    "prebuild": "node scripts/copy-env-to-build.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "analyze": "source-map-explorer build/static/js/*.js",
    "generate:corpus": "node scripts/generateCorpus.js",
    "train:agent": "ts-node src/agents/trainAgent.ts",
    "cap:sync": "npm run build && npx cap sync android",
    "cap:open": "npx cap open android",
    "cap:run": "npm run cap:sync && npm run cap:open",
    "open:browser": "pwsh -ExecutionPolicy Bypass -File scripts/open-browser.ps1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
